const colors= ["teal", "red", "blue", "orange"];
const handle= ["sunshine", "supergirl", "jaybee"];
const collection1 = colors.map(function(item,i){
  var handles =handle[i]
  return {index:i, color:item, handles:handles}
})
console.log(collection1);


const nums = [1,2,3];
const nums1 = nums.filter(num => num % 2 === 1);
console.log(nums1);
