    <script>

    
$(document).ready(function(){
  $( "#panda" ).click(function() {
  var question = prompt( "Do you like pandas?" );
    console.log(question);
  if ( question === "yes" ) {
    alert(" Me too!  ")
    console.log(":)")
  } else {
    $("#panda").hide();
    console.log(" :( ")
  }
  });
  });
    </script>

</head>
